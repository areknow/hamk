{
  "appId": "cr.areknow.hamk",
  "appName": "hamk",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com"
      ],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": [
          "profile"
        ]
      }
    },
    "Keyboard": {
      "resize": "none",
      "style": "dark"
    }
  },
  "cordova": {},
  "server": {
    "url": "http://192.168.1.228:8100",
    "cleartext": true
  }
}
